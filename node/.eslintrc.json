{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "airbnb-base", "prettier"],
    "globals": {},
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-bitwise": "off",
        "import/extensions": "always"
    },
    "ignorePatterns": ["node_modules/", "build/"]
}
